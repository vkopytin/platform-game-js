{
  "name": "databindjs",
  "version": "1.0.23",
  "description": "Two way data binding in javascript",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "prepare": "yarn build",
    "build": "npm --version",
    "lint": "tslint -p tsconfig.json",
    "test": "jest",
    "srv-dev": "node webpack.devserver.config -d --history-api-fallback--hot --inline --progress --colors",
    "dev": "cross-env NODE_OPTIONS=--max_old_space_size=4000 NODE_ENV=development webpack --watch --progress --color --config webpack.backend.config",
    "start": "node webpack.devserver.config -d --history-api-fallback--hot --inline --progress --colors",
    "make-ssl-sert": "openssl req -x509 -nodes -days 365 -newkey rsa:2048 -keyout ./selfsigned.key -out selfsigned.crt"
  },
  "keywords": [
    "javascript"
  ],
  "author": "Volodymyr Kopytin",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.json"
      }
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/backbone": "^1.4.1",
    "@types/express": "^4.17.6",
    "@types/jest": "^24.0.18",
    "@types/jquery": "^3.3.31",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/underscore": "^1.9.2",
    "@types/webpack": "^4.39.1",
    "babel-loader": "^8.1.0",
    "cookie-parser": "^1.4.5",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "helmet": "^3.23.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "method-override": "^3.0.0",
    "node-sass": "^4.13.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "prettier": "^1.18.2",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.5",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "fontsource-roboto": "^3.0.3",
    "lodash": "^4.17.20",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.4",
    "redux-observable": "^1.2.0",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.6.3"
  }
}
